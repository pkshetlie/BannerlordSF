<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link href="https://fonts.googleapis.com/css2?family=Montserrat&family=Open+Sans+Condensed:wght@300&display=swap" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
    {% include 'frontend/base_template/navbar.html.twig' %}
        {% block HtmlBody %}
            {% block body %}
            {% endblock %}
            <div id="btn-scroll-top"><i class="fa fa-angle-up"></i></div>
            {% include 'frontend/base_template/footer.html.twig' %}
        {% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
